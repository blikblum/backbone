{
  "name": "nextbone",
  "version": "0.3.0",
  "description": "BackboneJS rewritten with ES Classes.",
  "url": "http://backbonejs.org",
  "keywords": [
    "model",
    "view",
    "controller",
    "router",
    "server",
    "client",
    "browser"
  ],
  "author": "Jeremy Ashkenas",
  "dependencies": {
    "underscore": ">=1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "docco": "0.7.0",
    "eslint": "^4.19.1",
    "karma": "^2.0.2",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-qunit": "^2.1.0",
    "karma-rollup-preprocessor": "^6.0.0",
    "mocha": "^5.2.0",
    "qunit": "^2.6.1",
    "rollup": "^0.59.4",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "sinon": "^7.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "test": "karma start && npm run lint",
    "test:debug": "karma start --browsers ChromeDebugging --no-single-run",
    "test:computed": "cross-env NODE_ENV=test mocha --require babel-core/register test/computed",
    "test:fetch": "cross-env NODE_ENV=test mocha --require babel-core/register test/fetch",
    "test:localstorage": "karma start test/localstorage/karma.conf.js",
    "doc": "docco backbone.js && docco examples/todos/todos.js examples/backbone.localStorage.js",
    "lint": "eslint backbone.js computed.js test/*.js"
  },
  "sideEffects": false,
  "main": "backbone.js",
  "module": "backbone.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jashkenas/backbone.git"
  },
  "files": [
    "backbone.js",
    "LICENSE"
  ]
}
