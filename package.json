{
  "name": "nextbone",
  "version": "0.19.1",
  "description": "BackboneJS with ES classes and web components",
  "url": "https://github.com/blikblum/nextbone",
  "keywords": [
    "model",
    "view",
    "controller",
    "router",
    "server",
    "client",
    "browser"
  ],
  "author": "Luiz Américo Pereira Câmara",
  "dependencies": {
    "underscore": ">=1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "@open-wc/testing-helpers": "^0.7.22",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "docco": "0.8.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "jquery": "^3.3.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-qunit": "^3.0.0",
    "karma-rollup-preprocessor": "^7.0.0",
    "lit-element": "^2.0.1",
    "lit-html": "^1.0.0",
    "mocha": "^6.0.2",
    "prettier": "^1.16.4",
    "qunit": "^2.9.2",
    "rollup": "1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "test": "karma start test/core/karma.conf.js",
    "test:computed": "cross-env NODE_ENV=test mocha --require @babel/register test/computed",
    "test:fetch": "cross-env NODE_ENV=test mocha --require @babel/register test/fetch",
    "test:localstorage": "karma start test/localstorage/karma.conf.js",
    "test:validation": "cross-env NODE_ENV=test mocha --require @babel/register --ui exports --recursive test/validation",
    "test:virtualcollection": "karma start test/virtualcollection/karma.conf.js",
    "test:form": "karma start test/form/karma.conf.js",
    "test:class-utils": "cross-env NODE_ENV=test mocha --require @babel/register test/class-utils",
    "test:all": "npm run test && npm run test:computed && npm run test:fetch && npm run test:localstorage && npm run test:validation && npm run test:virtualcollection && npm run test:form && npm run test:class-utils",
    "doc": "docco backbone.js && docco examples/todos/todos.js examples/backbone.localStorage.js",
    "lint": "eslint nextbone.js computed.js localstorage.js virtualcollection.js form.js test/core/*.js",
    "format": "prettier nextbone.js computed.js localstorage.js virtualcollection.js form.js test/**/*.js --write"
  },
  "sideEffects": false,
  "main": "nextbone.js",
  "module": "nextbone.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/nextbone.git"
  },
  "files": [
    "nextbone.js",
    "computed.js",
    "localstorage.js",
    "validation.js",
    "virtualcollection.js",
    "form.js",
    "class-utils.js",
    "dom-utils.js",
    "LICENSE"
  ]
}
