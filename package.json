{
  "name": "nextbone",
  "version": "0.7.2",
  "description": "BackboneJS with ES classes and web components",
  "url": "https://github.com/blikblum/nextbone",
  "keywords": [
    "model",
    "view",
    "controller",
    "router",
    "server",
    "client",
    "browser"
  ],
  "author": "Jeremy Ashkenas",
  "dependencies": {
    "underscore": ">=1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@open-wc/testing-helpers": "^0.7.12",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "docco": "0.8.0",
    "eslint": "^5.13.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-qunit": "^2.1.0",
    "karma-rollup-preprocessor": "^6.1.2",
    "lit-element": "^2.0.0-rc.5",
    "lit-html": "^1.0.0-rc.2",
    "mocha": "^5.2.0",
    "qunit": "^2.9.1",
    "rollup": "0.68.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "sinon": "^7.2.3",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "test": "karma start",
    "test:debug": "karma start --browsers ChromeDebugging --no-single-run",
    "test:computed": "cross-env NODE_ENV=test mocha --require @babel/register test/computed",
    "test:fetch": "cross-env NODE_ENV=test mocha --require @babel/register test/fetch",
    "test:localstorage": "karma start test/localstorage/karma.conf.js",
    "test:validation": "cross-env NODE_ENV=test mocha --require @babel/register --ui exports --recursive test/validation",
    "test:virtualcollection": "cross-env NODE_ENV=test mocha --require @babel/register test/virtualcollection",
    "test:all": "npm run test && npm run test:computed && npm run test:fetch && npm run test:localstorage && npm run test:validation && npm run test:virtualcollection",
    "doc": "docco backbone.js && docco examples/todos/todos.js examples/backbone.localStorage.js",
    "lint": "eslint backbone.js computed.js localstorage.js virtualcollection.js test/core/*.js"
  },
  "sideEffects": false,
  "main": "nextbone.js",
  "module": "nextbone.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/nextbone.git"
  },
  "files": [
    "nextbone.js",
    "computed.js",
    "localstorage.js",
    "validation.js",
    "virtualcollection.js",
    "LICENSE"
  ]
}
